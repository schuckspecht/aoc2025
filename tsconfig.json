{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "rootDir": ".",
    "outDir": "./dist",
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true
  },
  "include": ["*"]
}
